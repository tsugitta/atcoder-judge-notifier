{
  "manifest_version": 2,
  "name": "AtCoder judge notifier",
  "description": "A Chrome extension to notify the result of the submission on AtCoder",
  "version": "0.1.0",
  "permissions": [
    "notifications",
    "tabs"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": ["https://atcoder.jp/contests/*/submissions/me"],
      "js": ["content-scripts.js"]
    }
  ]
}
