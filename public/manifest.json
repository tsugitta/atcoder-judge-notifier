{
  "manifest_version": 2,
  "name": "AtCoder judge notifier",
  "description": "__MSG_ext_description__",
  "version": "0.2.0",
  "default_locale": "en",
  "permissions": ["notifications", "tabs"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": ["https://atcoder.jp/contests/*/submissions/me"],
      "js": ["content-scripts.js"]
    }
  ]
}
